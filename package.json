{
  "name": "wavi-getx-awesome",
  "displayName": "Wavi GetX Awesome",
  "description": "Wavi GetX Awesome Extension!",
  "publisher": "MartinSilvaMolina",
  "version": "1.0.3",
  "engines": {
    "vscode": "^1.56.0"
  },
  "homepage": "https://github.com/msilvamolina/vscode-wavi-getx-awesome/blob/main/README.md",
  "categories": [
    "Other"
  ],
  "bugs": {
    "url": "https://github.com/msilvamolina/vscode-wavi-getx-awesome/issues",
    "email": "waviacademy@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/msilvamolina/vscode-wavi-getx-awesome/"
  },
  "icon": "icon.png",
  "activationEvents": [
    "onCommand:wavi-getx-awesome.init-app",
    "onCommand:wavi-getx-awesome.new-page",
    "onCommand:wavi-getx-awesome.new-model",
    "onCommand:wavi-getx-awesome.new-failure",
    "onCommand:wavi-getx-awesome.new-repository",
    "onCommand:wavi-getx-awesome.freezed-command"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "wavi-getx-awesome.init-app",
        "title": "Wavi GetX Initialize App"
      },
      {
        "command": "wavi-getx-awesome.new-page",
        "title": "Wavi GetX New Page"
      },
      {
        "command": "wavi-getx-awesome.new-model",
        "title": "Wavi GetX New Model"
      },
      {
        "command": "wavi-getx-awesome.new-failure",
        "title": "Wavi GetX New Failure"
      },
      {
        "command": "wavi-getx-awesome.new-repository",
        "title": "Wavi GetX New Repositoy"
      },
      {
        "command": "wavi-getx-awesome.freezed-command",
        "title": "Wavi GetX Freezed Command"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.56.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "14.x",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  }
}